          <TableColumn fx:id="colOId" prefWidth="154.0" text="Order Id" />
          <TableColumn fx:id="colItemCode" prefWidth="143.0" text="Item Code" />
            <TableColumn fx:id="colQty" prefWidth="258.0" text="Order Qty" />
            <TableColumn fx:id="colDiscount" prefWidth="159.0" text="Discount" />
            <TableColumn fx:id="colTotal" prefWidth="115.0" text="Total" />
