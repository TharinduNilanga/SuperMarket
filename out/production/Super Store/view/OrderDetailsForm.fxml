            <Font name="ROGFonts-Regular" size="36.0" />
         </font>
      </Label>
      <Separator layoutX="14.0" layoutY="54.0" prefHeight="17.0" prefWidth="919.0" />
      <TableView fx:id="tblOrderDetails" layoutX="64.0" layoutY="110.0" prefHeight="340.0" prefWidth="830.0">
